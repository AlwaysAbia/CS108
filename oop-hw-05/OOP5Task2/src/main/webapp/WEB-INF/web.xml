<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <listener>
    <listener-class>Task2Classes.SessionListener</listener-class>
  </listener>

  <servlet>
    <servlet-name>ServletAddToCart</servlet-name>
    <servlet-class>Task2Classes.ServletAddToCart</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>ServletUpdateCart</servlet-name>
    <servlet-class>Task2Classes.ServletUpdateCart</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ServletAddToCart</servlet-name>
    <url-pattern>/ServletAddToCart</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ServletUpdateCart</servlet-name>
    <url-pattern>/ServletUpdateCart</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
    <welcome-file>homePage.jsp</welcome-file>
  </welcome-file-list>
</web-app>
